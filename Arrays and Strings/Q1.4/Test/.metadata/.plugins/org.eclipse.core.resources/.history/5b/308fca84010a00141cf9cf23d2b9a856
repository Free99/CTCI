public class replace {
	public static String replaceSpace(String s) {
		if (s == null) return null;
		int len = s.length();
		if (len == 0) return null;
		int count = 0;
		for (int i = 0; i < len; ++i) {
			if (s.charAt(i) == ' ')
				++count;
		}
		
		char[] cc = new char[len + count * 2 + 1];
		int p = 0;
		
	}
}
